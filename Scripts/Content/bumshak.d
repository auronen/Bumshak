instance spell_windfist(c_spell_proto) {
    time_per_mana = 30.0;
    damage_per_level = spl_damage_windfist;
    damagetype = dam_fly;
    canturnduringinvest = true;
    targetcollectalgo = target_collect_focus_fallback_none;
    targetcollectrange = 1000;
    targetcollecttype = target_type_npcs;
};

func int spell_logic_windfist(var int manainvested) {
    if (self.attribute[atr_mana] /*2*/ < step_windfist) {
        return spl_dontinvest;
    };
    if (manainvested <= (step_windfist * 1)) {
        self.aivar[88] = 1;
        return spl_status_caninvest_no_manadec;
    }
    else if ((manainvested > (step_windfist * 1)) && (self.aivar[88] <= 1)) {
        self.attribute[atr_mana] /*2*/ = self.attribute[atr_mana] /*2*/ - step_windfist;
        if (self.attribute[atr_mana] /*2*/ < 0) {
            self.attribute[atr_mana] /*2*/ = 0;
        };
        self.aivar[88] = 2;
        return spl_nextlevel;
    }
    else if ((manainvested > (step_windfist * 2)) && (self.aivar[88] <= 2)) {
        self.attribute[atr_mana] /*2*/ = self.attribute[atr_mana] /*2*/ - step_windfist;
        if (self.attribute[atr_mana] /*2*/ < 0) {
            self.attribute[atr_mana] /*2*/ = 0;
        };
        self.aivar[88] = 3;
        return spl_nextlevel;
    }
    else if ((manainvested > (step_windfist * 3)) && (self.aivar[88] <= 3)) {
        self.attribute[atr_mana] /*2*/ = self.attribute[atr_mana] /*2*/ - step_windfist;
        if (self.attribute[atr_mana] /*2*/ < 0) {
            self.attribute[atr_mana] /*2*/ = 0;
        };
        self.aivar[88] = 4;
        return spl_nextlevel;
    }
    else {
        if ((manainvested > (step_windfist * 3)) && (self.aivar[88] == 4)) {
            return spl_dontinvest;
        };
    };
    return spl_status_caninvest_no_manadec;
};

func void spell_cast_windfist(var int spelllevel) {
    self.attribute[atr_mana] /*2*/ = self.attribute[atr_mana] /*2*/ - step_windfist;
    if (self.attribute[atr_mana] /*2*/ < 0) {
        self.attribute[atr_mana] /*2*/ = 0;
    };
    self.aivar[20] += 1;
};

