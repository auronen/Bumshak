func void b_addfightskill(var c_npc slf, var int talent, var int percent) {
    if (talent == npc_talent_1h) {
        slf.hitchance[npc_talent_1h] /*1*/ = slf.hitchance[npc_talent_1h] /*1*/ + percent;
        if (slf.hitchance[npc_talent_1h] /*1*/ >= 0) {
            npc_settalentskill(slf, npc_talent_1h, 0);
        };
        if (slf.hitchance[npc_talent_1h] /*1*/ >= 30) {
            npc_settalentskill(slf, npc_talent_1h, 1);
        };
        if (slf.hitchance[npc_talent_1h] /*1*/ >= 60) {
            npc_settalentskill(slf, npc_talent_1h, 2);
        };
    };
    if (talent == npc_talent_2h) {
        slf.hitchance[npc_talent_2h] /*2*/ = slf.hitchance[npc_talent_2h] /*2*/ + percent;
        if (slf.hitchance[npc_talent_1h] /*1*/ >= 0) {
            npc_settalentskill(slf, npc_talent_2h, 0);
        };
        if (slf.hitchance[npc_talent_2h] /*2*/ >= 30) {
            npc_settalentskill(slf, npc_talent_2h, 1);
        };
        if (slf.hitchance[npc_talent_2h] /*2*/ >= 60) {
            npc_settalentskill(slf, npc_talent_2h, 2);
        };
    };
    if (talent == npc_talent_bow) {
        slf.hitchance[npc_talent_bow] /*3*/ = slf.hitchance[npc_talent_bow] /*3*/ + percent;
        if (slf.hitchance[npc_talent_bow] /*3*/ >= 0) {
            npc_settalentskill(slf, npc_talent_bow, 0);
        };
        if (slf.hitchance[npc_talent_bow] /*3*/ >= 30) {
            npc_settalentskill(slf, npc_talent_bow, 1);
        };
        if (slf.hitchance[npc_talent_bow] /*3*/ >= 60) {
            npc_settalentskill(slf, npc_talent_bow, 2);
        };
    };
    if (talent == npc_talent_crossbow) {
        slf.hitchance[npc_talent_crossbow] /*4*/ = slf.hitchance[npc_talent_crossbow] /*4*/ + percent;
        if (slf.hitchance[npc_talent_crossbow] /*4*/ >= 0) {
            npc_settalentskill(slf, npc_talent_crossbow, 0);
        };
        if (slf.hitchance[npc_talent_crossbow] /*4*/ >= 30) {
            npc_settalentskill(slf, npc_talent_crossbow, 1);
        };
        if (slf.hitchance[npc_talent_crossbow] /*4*/ >= 60) {
            npc_settalentskill(slf, npc_talent_crossbow, 2);
        };
    };
};

func void b_raiserealfighttalentpercent(var c_npc oth, var int talent, var int percent) {
    if (talent == npc_talent_1h) {
        oth.aivar[84] = oth.aivar[84] + percent;
    }
    else if (talent == npc_talent_2h) {
        oth.aivar[85] = oth.aivar[85] + percent;
    }
    else if (talent == npc_talent_bow) {
        oth.aivar[86] = oth.aivar[86] + percent;
    }
    else {
        if (talent == npc_talent_crossbow) {
            oth.aivar[87] = oth.aivar[87] + percent;
        };
    };
};

func void b_raisefighttalent(var c_npc oth, var int talent, var int percent) {
    b_raiserealfighttalentpercent(oth, talent, percent);
    b_addfightskill(oth, talent, percent);
};

func void b_setfightskills(var c_npc slf, var int percent) {
    b_raisefighttalent(slf, npc_talent_1h, percent);
    b_raisefighttalent(slf, npc_talent_2h, percent);
    b_raisefighttalent(slf, npc_talent_bow, percent);
    b_raisefighttalent(slf, npc_talent_crossbow, percent);
};

func void b_logentry(var string topic, var string entry) {
    log_addentry(topic, entry);
    printscreen(print_newlogentry, -1, ypos_logentry, font_screensmall, 2);
    snd_play("LogEntry");
};

func void b_startotherroutine(var c_npc slf, var string newroutine) {
    ai_standup(slf);
    if (hlp_getinstanceid(self) != hlp_getinstanceid(slf)) {
        if (hlp_isvalidnpc(slf) && (!npc_isdead(slf))) {
            npc_exchangeroutine(slf, newroutine);
            ai_continueroutine(slf);
        };
    }
    else {
        npc_exchangeroutine(slf, newroutine);
    };
};

func void b_say(var c_npc slf, var c_npc oth, var string text) {
    ai_outputsvm(slf, oth, text);
};

func void b_say_overlay(var c_npc slf, var c_npc oth, var string text) {
    ai_outputsvm_overlay(slf, oth, text);
};

func void b_killnpc(var int npcinstance) {
    var c_npc npc;

    npc = hlp_getnpc(npcinstance);
    if (hlp_isvalidnpc(npc) && (!npc_isdead(npc))) {
        npc.flags = 0;
        npc_changeattribute(npc, atr_hitpoints, -npc.attribute[atr_hitpoints_max] /*1*/);
    };
};

func void b_removenpc(var int npcinstance) {
    var c_npc npc;

    npc = hlp_getnpc(npcinstance);
    if (hlp_isvalidnpc(npc) && (!npc_isdead(npc))) {
        npc.flags = 0;
        ai_teleport(npc, "TOT");
        b_startotherroutine(npc, "TOT");
        npc_changeattribute(npc, atr_hitpoints, -npc.attribute[atr_hitpoints_max] /*1*/);
        ai_teleport(npc, "TOT");
    };
};

